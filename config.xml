<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.uvmate" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MWORLD-R2</name>
    <description>
        mworld beta using preload
    </description>
    <author email="jeuelc@yahoo.com" href="http://cordova.io">
        Jeuel Condahan
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
</widget>
